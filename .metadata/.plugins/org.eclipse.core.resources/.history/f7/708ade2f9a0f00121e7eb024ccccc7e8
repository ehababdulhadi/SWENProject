package gameWorld;

public class Door {
	
	private Room room1;
	private Room room2;
	private boolean isLocked;
	private String doorKey;
	
	public Door (Room room, Room otherRoom, boolean locked, String keyName){
		
		this.room1 = room;
		this.room2 = otherRoom;
		this.isLocked = locked;
		this.doorKey = keyName;
	}

}
